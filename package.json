  },
  "devDependencies": {
    "chai": "^3.5.0",
    "database-cleaner": "^1.1.0",
    "eslint": "^2.7.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^2.4.5",
    "should": "^11.1.1",
    "supertest": "^1.2.0"
  }
}
