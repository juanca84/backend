{
  "name": "simm-portal_mineria",
  "version": "1.0.0",
  "description": "Examen Backend",
  "author": "Juan Carlos Condori Machicado",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "startdev": "nodemon --exec 'node index.js'",
    "test": "NODE_ENV=test FORCE=true node index.js && sequelize db:seed:all --env test --seeders-path src/seeders/ && sequelize db:migrate:undo:all --env test --migrations-path src/migrations && sequelize db:migrate --env test --migrations-path src/migrations && NODE_ENV=test mocha --timeout 30000 test/**/**/*.js",
    "lint": "eslint *.js, src/**/*.js, src/**/**/*.js",
    "apidoc": "apidoc -i src/routes/ -o public/apidoc",
    "setup": "FORCE=true node index.js && sequelize db:seed:all --seeders-path src/seeders && sequelize db:migrate:undo:all --migrations-path src/migrations && sequelize db:migrate --migrations-path src/migrations",
    "coverage": "NODE_ENV=test istanbul cover node_modules/mocha/bin/_mocha -- test/**/**/*.js",
    "parchar": "node parches/parchar.js",
    "instalar": "npm install && node parches/parchar.js",
    "seeders": "sequelize db:seed:all --seeders-path src/seeders",
    "functions": "sequelize db:migrate:undo:all --migrations-path src/migrations && sequelize db:migrate --migrations-path src/migrations"
  },
  "keywords": [
    "node",
    "base",
    "Ministerio de Mineria",
    "SIMM",
    "Portal Mineria"
  ],
  "apidoc": {
    "name": "API REST - Documentacion",
    "template": {
      "forceLanguage": "es"
    }
  },
  "author": "agetic-uiid",
  "license": "MIT",
  "dependencies": {
    "base-64": "^0.1.0",
    "bcrypt": "^0.8.5",
    "body-parser": "^1.15.0",
    "chai": "^3.5.0",
    "consign": "^0.1.2",
    "cors": "^2.7.1",
    "crypto": "0.0.3",
    "csvtojson": "^1.1.6",
    "ejs": "^2.4.1",
    "express": "^4.14.0",
    "express-fileupload": "^0.1.3",
    "express-http-proxy": "^0.10.1",
    "formidable": "^1.0.17",
    "hashids": "^1.1.1",
    "helmet": "^3.4.1",
    "html-pdf": "^2.1.0",
    "jimp": "^0.2.28",
    "jsonwebtoken": "^7.2.1",
    "jsreport": "^1.7.0",
    "jsreport-core": "^1.3.2",
    "jwt-simple": "^0.5.0",
    "konfig": "^0.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "morgan": "^1.7.0",
    "mysql": "^2.10.2",
    "node-rest-client": "^1.8.0",
    "nodemailer": "^4.4.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "nodemon": "^1.12.5",
    "passport": "^0.3.2",
    "passport-jwt": "^2.0.0",
    "passport-ldapauth": "^0.5.0",
    "path": "^0.12.7",
    "pg": "^4.5.6",
    "phantomjs": "^2.1.7",
    "phantomjs-prebuilt": "^2.1.13",
    "q": "^1.4.1",
    "qr-image": "^3.2.0",
    "request": "^2.78.0",
    "request-promise": "^4.2.1",
    "sequelize": "^3.31.1",
    "sequelize-cli": "^3.2.0",
